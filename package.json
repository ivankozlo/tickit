{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.3",
    "esbuild": "^0.14.48",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-trello": "^2.2.11",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --loader:.js=jsx",
    "watch": "esbuild app/javascript/*.* --watch --bundle --outdir=app/assets/builds --loader:.js=jsx --define:process.env.REDUX_LOGGING=true"
  }
}
